<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    tools:context=".ui.home.HomeFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/lottie_view8"
                android:layout_width="0dp"
                android:layout_height="400px"
                android:layout_weight="1"
                android:scaleX="1.8"
                android:scaleY="1.8"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"
                app:lottie_rawRes="@raw/swinging" />

            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/lottie_view"
                android:layout_width="0dp"
                android:layout_height="400px"
                android:layout_weight="1"
                android:rotation="360"
                android:scaleX="-1.8"
                android:scaleY="1.8"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"
                app:lottie_rawRes="@raw/swinging" />
        </LinearLayout>


        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_view6"
            android:layout_width="match_parent"
            android:layout_height="400px"
            android:scaleX="2"
            android:scaleY="2"
            app:lottie_autoPlay="true"
            android:rotation="180"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/swinging" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_view2"
            android:layout_width="match_parent"
            android:layout_height="400px"
            android:scaleX="2"
            android:scaleY="2"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/swinging" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_view1"
            android:layout_width="match_parent"
            android:layout_height="400px"
            android:scaleX="2"
            android:scaleY="2"
            android:rotation="180"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/swinging" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_view11"
            android:layout_width="match_parent"
            android:layout_height="400px"
            android:scaleX="2"
            android:scaleY="2"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/swinging" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_view132"
            android:layout_width="match_parent"
            android:layout_height="400px"
            android:scaleX="2"
            android:scaleY="2"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/swinging" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_view13"
            android:layout_width="match_parent"
            android:layout_height="400px"
            android:scaleX="2"
            android:rotation="180"
            android:scaleY="2"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/swinging" />

    </LinearLayout>
</androidx.core.widget.NestedScrollView>
